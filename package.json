{
  "name": "vk-bridge-level",
  "version": "1.1.1",
  "description": "Level abstraction for VK Bridge",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": ["lib/**/*"],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare" : "npm run build",
    "prepublishOnly" : "npm run lint",
    "preversion" : "npm run lint",
    "version" : "npm run format && git add -A src",
    "postversion" : "git push && git push --tags"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1",
    "prettier": "^3",
    "tslint": "^6",
    "tslint-config-prettier": "^1",
    "typescript": "^5"
  },
  "dependencies": {
    "@vkontakte/vk-bridge": "^2",
    "abstract-level": "^1",
    "module-error": "^1"
  },
  "keywords": ["birdge", "level"],
  "author": "Mnwa",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mnwa/vk-bridge-level.git"
  }
}
